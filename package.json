    "@react-navigation/stack": "^5.9.0",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "@ui-kitten/moment": "^5.0.0",
    "axios": "^0.19.2",
    "currency.js": "^2.0.3",
    "date-fns": "^2.15.0",
    "formik": "^2.1.5",
    "moment": "^2.27.0",
    "numeral": "^2.0.6",
